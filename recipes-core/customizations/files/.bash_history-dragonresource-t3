jailhouse enable /etc/jailhouse/dragonresource-t3.cell
jailhouse console
jailhouse cell create /etc/jailhouse/dragonresource-t3-inmate-demo.cell
jailhouse cell load dragonresource-t3-inmate-demo \
                    /usr/libexec/jailhouse/demos/gic-demo.bin \
                    -s "led-reg=0x1c20810 led-pin=17" -a 0x1000
jailhouse cell start dragonresource-t3-inmate-demo
jailhouse cell stats dragonresource-t3-inmate-demo
jailhouse cell destroy dragonresource-t3-inmate-demo
jailhouse cell linux /etc/jailhouse/dragonresource-t3-linux-demo.cell /boot/vmlinuz* \
                     -d /etc/jailhouse/dts/inmate-dragonresource-t3.dtb \
                     -i /usr/libexec/jailhouse/demos/rootfs.cpio \
                     -c "console=ttyS0,115200 ip=192.168.19.2"
ssh 192.168.19.2
jailhouse disable
